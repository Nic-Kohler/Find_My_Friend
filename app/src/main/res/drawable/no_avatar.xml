			         android:bottomLeftRadius="10dp"
			         android:topLeftRadius="10dp"
			         android:topRightRadius="10dp"/>
		</shape>
	</item>

	<item android:top="40dp" android:left="9dp" android:right="9dp" android:bottom="0dp">
		<shape android:shape="rectangle">
			<size android:width="32dp"
			      android:height="10dp"/>
			<solid android:color="@color/color_1"/>
		</shape>
	</item>
</layer-list>