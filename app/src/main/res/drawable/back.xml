<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:top="0dp" android:left="0dp" android:bottom="0dp" android:right="0dp">
		<shape android:shape="rectangle">
			<size android:width="20dp"
			      android:height="27dp" />
		</shape>
	</item>
	<item android:top="-5dp" android:bottom="16dp" android:left="3dp" android:right="11dp">
		<rotate
			android:fromDegrees="45"
			android:toDegrees="45"
			android:pivotX="0%"
			android:pivotY="100%" >
			<shape android:shape="rectangle">
				<solid android:color="@color/color_3" />
				<corners android:radius="5dp" />
			</shape>
		</rotate>
	</item>
	<item android:top="16dp" android:bottom="-6dp" android:left="2dp" android:right="12dp">
		<rotate
			android:fromDegrees="-45"
			android:toDegrees="-45"
			android:pivotX="0%"
			android:pivotY="0%" >
			<shape android:shape="rectangle">
				<solid android:color="@color/color_3" />
				<corners android:radius="5dp" />
			</shape>
		</rotate>
	</item>
	<item android:top="7dp" android:bottom="13dp" android:left="0dp" android:right="15dp">
		<rotate
			android:fromDegrees="45"
			android:toDegrees="45"
			android:pivotX="0%"
			android:pivotY="100%" >
			<shape android:shape="rectangle">
				<solid android:color="@color/color_3" />
			</shape>
		</rotate>
	</item>
</layer-list>